doctype html
html
    head
        meta(charset="utf-8")
        title Library
        link(rel='stylesheet', href='/src/style/main.css')
    body
        header
            .header_name Библиотека
        div
            ul
                each book in books
                    li
                        a(href="/books/" + book.id) 
                            =`${book.id}: ${book.author} . ${book.title}`